local fake_module_scripts = {}


-- Fake Local Scripts:

local function HMUY_fake_script() -- Fake Script: StarterGui.DHSALTCONTROL.Frame.ImageLabel.LocalScript
	local Players = game:GetService("Players")
	local Player = game.Players.LocalPlayer
	
	local content = Players:GetUserThumbnailAsync(Player.UserId, Enum.ThumbnailType.HeadShot, Enum.ThumbnailSize.Size420x420)
	Converted["_ImageLabel1"].Image = content
end
local function FTHAIGH_fake_script() -- Fake Script: StarterGui.DHSALTCONTROL.Frame.TextLabel.LocalScript
	local player = game.Players.LocalPlayer
	local username = player.Name
	local textLabel = Converted["_TextLabel"]
	
	textLabel.Text = username
end

coroutine.wrap(HMUY_fake_script)()
coroutine.wrap(FTHAIGH_fake_script)()

local function format(number)
    number = tostring(number)
    return number:reverse():gsub("...","%0,",math.floor((#number-1)/3)):reverse()
end

local function hms_format(Int)
    return string.format("%02i", Int)
end

local function convert_to_hms(seconds)
    local minutes = (seconds - seconds%60)/60
    seconds = seconds - minutes*60
    local hours = (minutes - minutes%60)/60
    minutes = minutes - hours*60
    return hms_format(hours)..":"..hms_format(minutes)..":"..hms_format(seconds)
end

local data_folder = game.Players.LocalPlayer:WaitForChild("DataFolder")
local timeelapsed = tick()
local startingcash = data_folder.Currency.Value

task.spawn(function()
    while task.wait() do
        pcall(function()
            Converted["_Total"].Text = "$"..format(data_folder.Currency.Value)
			local taxAmount = data_folder.Currency.Value * 0.30
			local fullamount = data_folder.Currency.Value - taxAmount
			Converted["_TaxTotal"].Text = fullamount
        end)
    end
end)

task.spawn(function()
    while task.wait() do
        pcall(function()
            Converted["_Total1"].Text = "$"..format(startingcash - data_folder.Currency.Value)
        end)
    end
end)

