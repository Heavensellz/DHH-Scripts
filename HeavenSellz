local lastPlatform = nil

local function createPlatform(position)
    if lastPlatform then
        lastPlatform:Destroy()
    end

    local platform = Instance.new("Part")
    platform.Size = Vector3.new(10, 1, 10)
    platform.Position = position - Vector3.new(0, 1, 0)
    platform.Anchored = true
    platform.CanCollide = true
    platform.BrickColor = BrickColor.new("Bright blue")
    platform.Parent = game.Workspace

    lastPlatform = platform

    return platform
end

local function Setup(Type, Debugmode)
    if Debugmode then
        for PointInTable = 1, 40 do
            local Table = SetupsTable[Type]
            local position = Table["Positions"][PointInTable]
            
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = position
            local clone = game.Players.LocalPlayer.Character:Clone()
            clone.Parent = workspace
            clone.HumanoidRootPart.CFrame = position

			local humanoidRootPart = Player.Character.HumanoidRootPart
			humanoidRootPart.CFrame = position
        end
    else
		local Table = SetupsTable[Type]
        local PointInTable = getgenv().PointInTable
        if Table and Table["Positions"] and Table["Positions"][PointInTable] then
            local position = Table["Positions"][PointInTable]
			if airlock == true then
				local pos = position + CFrame.new(0, 15, 0)
				createPlatform(pos.Position)
				local humanoidRootPart = Player.Character.HumanoidRootPart
				humanoidRootPart.CFrame = pos
			elseif ground == true then
				local pos = position - CFrame.new(0, 8, 0)
				createPlatform(pos.Position)
				local humanoidRootPart = Player.Character.HumanoidRootPart
				humanoidRootPart.CFrame = pos
			else
				createPlatform(position.Position)
				local humanoidRootPart = Player.Character.HumanoidRootPart
				humanoidRootPart.CFrame = position
			end
        else
            warn("No position found for the given setup type and point in table.")
        end
    end
end

local function Drop(Type)
    if Type == true and drop == nil then
        drop = true
        while drop do
            local args = {
                [1] = "DropMoney",
                [2] = "15000"
            }
            game:GetService("ReplicatedStorage").MainEvent:FireServer(unpack(args))
            wait(2.5)
        end
    elseif Type == false then
        drop = nil
    end
end
local function ShowWallet()
	if Player.Backpack:FindFirstChild("[Wallet]") then
		Player.Character.Humanoid:EquipTool(Player.Backpack:FindFirstChild("[Wallet]"))
	end
end
local function RemoveWallet()
	if Player.Character:FindFirstChild("[Wallet]") then
		Player.Character.Humanoid:UnequipTools()
	end
end

local function block(type)
    if type == true then
        if block == nil or block == false then
            game.ReplicatedStorage.MainEvent:FireServer("Block", true)
            block = true
        end
    elseif type == false then
        if block == true then
            game.ReplicatedStorage.MainEvent:FireServer("Block", false)
            block = false
        end
    end
end

local function AirLock()
    local pos = lastPlatform.Position
    local airlockpos = pos + Vector3.new(0, 13.5, 0)

    createPlatform(airlockpos)
    Player.Character.HumanoidRootPart.Position = airlockpos + Vector3.new(0, 1, 0)
end

local function UnAirlock()
    local pos = lastPlatform.Position
    local airlockpos = pos - Vector3.new(0, 11.5, 0)

    createPlatform(airlockpos)
    Player.Character.HumanoidRootPart.Position = airlockpos + Vector3.new(0, 1, 0)
end

local function Ground()
    local pos = lastPlatform.Position
    local groundpos = pos - Vector3.new(0, 6.5, 0)

    createPlatform(groundpos)
    Player.Character.HumanoidRootPart.Position = groundpos + Vector3.new(0, 1, 0)
end

local function Surface()
    local pos = lastPlatform.Position
    local surfacepos = pos + Vector3.new(0, 8.5, 0)

    createPlatform(surfacepos)
    Player.Character.HumanoidRootPart.Position = surfacepos + Vector3.new(0, 1, 0)
end

local function airlockfunc()
	if airlock then
		return
	end
	if ground then
		ground = false
		Surface()
	end
	airlock = true
	AirLock()
end
local function unairlockfunc()
	if not airlock then
		return
	end
	if ground then
		ground = false
		airlock = false
		Surface()
		return
	end
	airlock = false
	UnAirlock()
end
local function groundfunc()
	if ground then
		return
	end
	if airlock then
		airlock = false
		UnAirlock()
	end
	ground = true
	Ground()
end
local function surfacefunc()
	if not ground then
		return
	end
	if airlock then
		airlock = false
		ground = false
		UnAirlock()
		return
	end
	ground = false
	Surface()
end

--// On inject functions
local function defaultsetup(type)
    msg("Setup "..type)
	Setup(type)
end

defaultsetup(getgenv().AltControlSetting.Settings.Misc.Setup)
    
if getgenv().AltControlSetting.Settings.Misc.Wallet == true then
	ShowWallet()
else
	return
end

if getgenv().AltControlSetting.Main.Ads == true then
	local ad = getgenv().AltControlSetting.Settings.Misc.AdSettings.Message
	task.spawn(function()
		while task.wait(getgenv().AltControlSetting.Settings.Misc.AdSettings.Interval * 60) do
			msg(ad)
		end
	end)
else
	return
end

local Connections = {}
local CmdSettings = {}

--// Listening to host or gui function
local function Initiate()
	CurrAnim = nil
	for Index,Var in pairs(CmdSettings) do
		CmdSettings[Var] = nil
	end
	CmdSettings = {}
	for Index,Connection in pairs(Connections) do
		Index[Connection] = nil
		Connection:Disconnect()
	end
	Connections["OnChat"] = game.ReplicatedStorage.DefaultChatSystemChatEvents.OnMessageDoneFiltering.OnClientEvent:Connect(function(data)
 		local Message = data.Message
		local plr = game:service"Players"[data.FromSpeaker]
		if plr.UserId ~= getgenv().AltControlSetting.Main.Host then
			return
		end
 		Message = string.lower(Message)
		local Args = string.split(Message," ")
		local AmountOfArgs = #Args
		local prefix = getgenv().AltControlSetting.Settings.Main.Prefix
		if Host and Player.Character and Player.Character:FindFirstChild("HumanoidRootPart") and Player.Character:FindFirstChild("Humanoid") and Player.Character.Humanoid.Health > 0 then
			if Args[1] == prefix.."drop" then
				Drop(true)
			elseif Args[1] == prefix.."stop" then
				Drop(false)
			elseif Message == prefix.."airlock" then
				airlockfunc()
			elseif Message == prefix.."unairlock" then
				unairlockfunc()

			elseif Message == prefix.."ground" then
				groundfunc()
			elseif Message == prefix.."surface" then
				surfacefunc()

			elseif Message == prefix.."redeem" then
				game:GetService("ReplicatedStorage").MainEvent:FireServer("EnterPromoCode", Args[1])

			elseif Args[1] == prefix.."block" then
				block(true)
			elseif Args[1] == prefix.."unblock" then
				block(false)

			elseif Message == prefix.."setup bank" then
				Setup("Bank")
			elseif Message == prefix.."setup court" then
				Setup("Court")
			elseif Message == prefix.."setup club" then
				Setup("Club")
			elseif Message == prefix.."setup train" then
				Setup("Train")

			elseif Message == prefix.."circle" and Host and Host.Character and Host.Character:FindFirstChild("Humanoid") and Host.Character.Humanoid.Health > 0 then
				local angle = 0
				local cfr = Host.Character.HumanoidRootPart.CFrame*CFrame.new(0,1,0)
				local Index = 0
				local size = 3

				local PointInTable = getgenv().PointInTable

				local ZAxis
				local angle = 0
				if PointInTable <= 10 then
					ZAxis = 2
					angle = (10-PointInTable)
				elseif PointInTable <= 20 then
					ZAxis = -1
					angle = (20-PointInTable)
				elseif PointInTable <= 30 then
					ZAxis = -4
					angle = (30-PointInTable)
				elseif PointInTable <= 40 then
					ZAxis = -8
					angle = (40-PointInTable)
				end
				angle = angle*36
				local Clone = game.Players.LocalPlayer.Character
				Clone.HumanoidRootPart.CFrame = cfr * CFrame.fromEulerAnglesXYZ(0,math.rad(angle),0) * CFrame.new(0,-size,-10)
				Clone.HumanoidRootPart.CFrame=Clone.HumanoidRootPart.CFrame*CFrame.new(0,0,2)
				Clone.HumanoidRootPart.CFrame=Clone.HumanoidRootPart.CFrame*CFrame.Angles(0,math.rad(180),0)

			elseif Message == prefix.."spot" then
				local hrp = game.Players.LocalPlayer
				local cfr = Host.Character.HumanoidRootPart.CFrame*CFrame.new(0,0,-5)
				hrp.Character.HumanoidRootPart.CanCollide = false

				local Clone = game.Players.LocalPlayer.Character
				Clone.HumanoidRootPart.CFrame = cfr

			elseif Message == prefix.."wallet on" then
				ShowWallet()
			elseif Message == prefix.."wallet off" then
				RemoveWallet()

			elseif Message == prefix.."floss" then
				if CurrAnim and CurrAnim.IsPlaying then
					CurrAnim:Stop()
				end
				local Anim = Instance.new("Animation")
				Anim.AnimationId = "http://www.roblox.com/asset/?id=10714340543"
				CurrAnim = game.Players.LocalPlayer.Character.Humanoid.Animator:LoadAnimation(Anim)
				CurrAnim:Play()
				CurrAnim:AdjustSpeed()

			elseif Message == prefix.."bonechill" then
				if CurrAnim and CurrAnim.IsPlaying then
					CurrAnim:Stop()
				end
				local Anim = Instance.new("Animation")
				Anim.AnimationId = "http://www.roblox.com/asset/?id=15122972413"
				CurrAnim = game.Players.LocalPlayer.Character.Humanoid.Animator:LoadAnimation(Anim)
				CurrAnim:Play()
				CurrAnim:AdjustSpeed()

			elseif Message == prefix.."fun" then
				if CurrAnim and CurrAnim.IsPlaying then
					CurrAnim:Stop()
				end
				local Anim = Instance.new("Animation")
				Anim.AnimationId = "http://www.roblox.com/asset/?id=12874447851"
				CurrAnim = game.Players.LocalPlayer.Character.Humanoid.Animator:LoadAnimation(Anim)
				CurrAnim:Play()
				CurrAnim:AdjustSpeed() 

			elseif Message == prefix.."dolphin" then
				if CurrAnim and CurrAnim.IsPlaying then
					CurrAnim:Stop()
				end
				local Anim = Instance.new("Animation")
				Anim.AnimationId = "http://www.roblox.com/asset/?id=10714068222"
				CurrAnim = game.Players.LocalPlayer.Character.Humanoid.Animator:LoadAnimation(Anim)
				CurrAnim:Play()
				CurrAnim:AdjustSpeed()
			elseif Message == prefix.."fly" then
				if CurrAnim and CurrAnim.IsPlaying then
					CurrAnim:Stop()
				end
				local Anim = Instance.new("Animation")
				Anim.AnimationId = "rbxassetid://13850660986"

				CurrAnim = game.Players.LocalPlayer.Character.Humanoid.Animator:LoadAnimation(Anim)
				CurrAnim:Play()
				CurrAnim:AdjustSpeed() 

			elseif Message == prefix.."sturdy" then
				if CurrAnim and CurrAnim.IsPlaying then
					CurrAnim:Stop()
				end
				local Anim = Instance.new("Animation")
				Anim.AnimationId = "rbxassetid://11710524717"

				CurrAnim = game.Players.LocalPlayer.Character.Humanoid.Animator:LoadAnimation(Anim)
				CurrAnim:Play()
				CurrAnim:AdjustSpeed() 

			elseif Message == prefix.."rossy" then
				if CurrAnim and CurrAnim.IsPlaying then
					CurrAnim:Stop()
				end
				local Anim = Instance.new("Animation")
				Anim.AnimationId = "rbxassetid://1171052744"

				CurrAnim = game.Players.LocalPlayer.Character.Humanoid.Animator:LoadAnimation(Anim)
				CurrAnim:Play()
				CurrAnim:AdjustSpeed() 

			elseif Message == prefix.."griddy" then
				if CurrAnim and CurrAnim.IsPlaying then
					CurrAnim:Stop()
				end
				local Anim = Instance.new("Animation")
				Anim.AnimationId = "rbxassetid://11710529220"

				CurrAnim = game.Players.LocalPlayer.Character.Humanoid.Animator:LoadAnimation(Anim)
				CurrAnim:Play()
				CurrAnim:AdjustSpeed() 

			elseif Message == prefix.."tpose" then
				if CurrAnim and CurrAnim.IsPlaying then
					CurrAnim:Stop()
				end
				local Anim = Instance.new("Animation")
				Anim.AnimationId = "rbxassetid://11710524200"

				CurrAnim = game.Players.LocalPlayer.Character.Humanoid.Animator:LoadAnimation(Anim)
				CurrAnim:Play()
				CurrAnim:AdjustSpeed() 

			elseif Message == prefix.."speed" then
				if CurrAnim and CurrAnim.IsPlaying then
					CurrAnim:Stop()
				end
				local Anim = Instance.new("Animation")
				Anim.AnimationId = "rbxassetid://11710541744"

				CurrAnim = game.Players.LocalPlayer.Character.Humanoid.Animator:LoadAnimation(Anim)
				CurrAnim:Play()
				CurrAnim:AdjustSpeed() 

			elseif Message == prefix.."stopdance" then
				if CurrAnim and CurrAnim.IsPlaying then
					CurrAnim:Stop()
				end
			end
		end
	end)
end
Initiate()

local function onPlayerAdded(player)
    -- Connect the player's Chatted event to the onPlayerChatted function
    player.Chatted:Connect(function(message)
		if player.Name:lower() ~= HostUser:lower() then
            detectalts(player, message)
		end
    end)
end

-- Connect the PlayerAdded event to the onPlayerAdded function
game.Players.PlayerAdded:Connect(onPlayerAdded)

-- Connect the existing players in case the script runs after they have joined
for _, player in ipairs(game.Players:GetPlayers()) do
    onPlayerAdded(player)
end
