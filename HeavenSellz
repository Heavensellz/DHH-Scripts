            Vector3.new(-404.5, 21.74 - 3, 88.09),
            Vector3.new(-496.04, 48 - 3, -594.2),
            Vector3.new(-142.5, 21.75 - 3, -823.2),
            Vector3.new(-321.5, 21.75 - 3, -448.79),
            Vector3.new(-208,21.75 - 3,-1131.96),
            Vector3.new(-377.35,12.74 - 3,-689.7),
            Vector3.new(-735.57,21.74 - 3,142.32),
            Vector3.new(-132.39,21.74 - 3,-231.05),
            Vector3.new(-810.19,21.74 - 3,-216.58),
            Vector3.new(211.81,48 - 3,-594.96),
            Vector3.new(8.14,21.75 - 3,-546.75),
            Vector3.new(494,48 - 3,-595.68),
            Vector3.new(-838.89,21.75 - 3,-620.58),
            Vector3.new(450,48 - 3,-402)
        }

        for _, position in ipairs(positions) do

            local platform = Instance.new("Part")
            platform.Size = Vector3.new(10, 1, 10)
            platform.Position = position
            platform.Anchored = true
            platform.BrickColor = BrickColor.new("Bright blue")
            platform.Parent = workspace
            platform.CanCollide = true

            platform.Transparency = 0.2
        end
    end

    createPlatformsAtPositions()
end)

--// Alt Control Communication
getgenv().PointInTable = 1

local alts = {}
local randomwords = {"eminent4", "subdued1", "flippant6", "vivacious2", "feigned4", "fallacious8"}

local function chatRandomMessages()
    local randomIndex = math.random(1, #randomwords)
    local randomMessage = randomwords[randomIndex]

    require(game:GetService("Players").LocalPlayer.PlayerScripts.ChatScript.ChatMain).MessagePosted:fire(randomMessage)
end

chatRandomMessages()

local function detectalts(player, message)
    for _, word in ipairs(randomwords) do
        if message == word then
            for _, id in ipairs(alts) do
                if id == player.UserId then
                    return
                end
            end

            table.insert(alts, player.UserId)
            getgenv().PointInTable = getgenv().PointInTable + 1
            return
        end
    end
end

local function removealtid(player)
    for index, id in ipairs(alts) do
        if id == player.UserId then
            table.remove(alts, index)
            getgenv().PointInTable = getgenv().PointInTable - 1
            break
        end
    end
end

game.Players.PlayerRemoving:Connect(removealtid)

--// Services/main variables
local Player = game.Players.LocalPlayer
local Players = game.Players
local HostUser = getgenv().AltControlSetting.Main.Host
local CHAT_EVENT = game.ReplicatedStorage:WaitForChild("DefaultChatSystemChatEvents"):WaitForChild("SayMessageRequest")
local Host = Players:FindFirstChild(HostUser)

--// command settings
local airlock = false
local ground = false    
local drop = nil
local customdrop = nil

--// Checks if host is ingame and if not it waits
if not Host then
	print("Host is not here waiting for him to join!")
	Players:WaitForChild(HostUser, 9e9)
	Host = Players:FindFirstChild(HostUser)
end

--// All Functions

local function msg(message)
	CHAT_EVENT:FireServer(message, "All")
end

